{
  "name": "Notifier for Clien",
  "version": "1.0.0",
  "description": "",
  "homepage_url": "https://github.com/cbcruk/webext",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "alarms",
    "storage",
    "notifications",
    "tabs",
    "cookies",
    "*://*.clien.net/"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon-toolbar.png",
    "default_title": "cliwn",
    "default_popup": "popup.html"
  }
}
